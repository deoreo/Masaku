<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="twiscode.masakuuser" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme"
        tools:replace="android:icon">

        <activity
            android:name=".Activity.ActivitySplash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.ActivityHome" />
        <activity android:name=".Activity.ActivityLogin" />
        <activity android:name=".Activity.ActivityRegister" />
        <activity android:name=".Activity.ActivityFeedback" />
        <activity android:name=".Activity.ActivityMenuDetail" />
        <activity android:name=".Activity.ActivityMenuDetailNew" />
        <activity android:name=".Activity.ActivityAbout" />
    </application>

</manifest>
